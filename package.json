{
  "name": "event-station",
  "version": "1.1.2",
  "author": "Morris Allison III <author@morris.xyz> (http://morris.xyz)",
  "description": "A versatile and robust event emitter class.",
  "license": "MIT",
  "homepage": "https://github.com/morrisallison/event-station",
  "bugs": "https://github.com/morrisallison/event-station/issues",
  "repository": "github:morrisallison/event-station",
  "keywords": [
    "async",
    "asynchronous",
    "class",
    "dispatcher",
    "emitter",
    "es6",
    "event",
    "eventemitter",
    "events",
    "fluent",
    "mixin",
    "promise",
    "promisified",
    "regexp",
    "rx",
    "standalone",
    "typescript"
  ],
  "directories": {
    "test": "tests"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^6.0.51",
    "benchmark": "^2.1.2",
    "bluebird": "^3.4.6",
    "istanbul": "~1.1.0-alpha.1",
    "livescript": "^1.5.0",
    "mkdirp": "^0.5.1",
    "mocha": "~2.5.3",
    "must": "^0.13.2",
    "npm-install-version": "^6.0.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.38.1",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rx": "^4.1.0",
    "ts-node": "~1.2.2",
    "typescript": "^2.1.4",
    "uglify-js": "^2.7.4"
  },
  "scripts": {
    "benchmark": "lsc benchmarks/event-callback.ls",
    "build": "npm run compile && npm run bundle && npm run optimize",
    "build:dts": "npm run compile:dts && npm run bundle:dts",
    "build:es5": "npm run compile:es5 && npm run bundle:es5",
    "build:es6": "npm run compile:es6 && npm run bundle:es6",
    "bundle": "npm run bundle:dts && npm run bundle:es5 && npm run bundle:es6",
    "bundle:dts": "node scripts/bundle-dts.js",
    "bundle:es5": "rollup --sourcemap -c scripts/rollup.config.es5.js",
    "bundle:es6": "rollup --sourcemap -c scripts/rollup.config.es6.js",
    "clean": "rimraf dist build coverage",
    "compile": "npm run compile:dts && npm run compile:es5 && npm run compile:es6",
    "compile:default": "tsc",
    "compile:dts": "tsc -p scripts/tsconfig.dts.json",
    "compile:es5": "tsc -p scripts/tsconfig.es5.json",
    "compile:es6": "tsc -p scripts/tsconfig.es6.json",
    "cover": "istanbul cover -e .ts _mocha -- --compilers ts:ts-node/register,ls:livescript --recursive tests --reporter min",
    "gamut": "npm run clean && npm run build && npm run validate && npm run test && npm run cover",
    "optimize": "uglifyjs --screw-ie8 --comments -c -m -o dist/event-station.min.js --source-map dist/event-station.min.js.map -p relative dist/event-station.js",
    "test": "mocha --compilers ts:ts-node/register,ls:livescript --recursive tests",
    "validate": "npm run validate:dts",
    "validate:dts": "tsc --lib 'es5','es2015.promise' dist/event-station.d.ts"
  },
  "main": "./dist/event-station.js",
  "jsnext:main": "./dist/event-station.jsnext.js",
  "typings": "./dist/event-station.d.ts",
  "jspm": {
    "format": "esm",
    "jspmNodeConversion": false,
    "main": "./dist/event-station.jsnext.js"
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "docs",
    "LICENSE",
    "README.md"
  ],
  "runkitExampleFilename": "./docs/runkit-example.js"
}
